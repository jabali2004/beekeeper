{
  "name": "beekeeper-frontend",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json --output ./src/assets/i18n/de.json --format namespaced-json  --marker . --sort",
    "openapi:update": "curl http://localhost:5000/swagger/v1/swagger.json > docs/backend.json && npm run openapi:generate",
    "openapi:generate": "java -jar ./openapi/openapi-generator-cli.jar generate -g typescript-angular -o ./src/app/api_client -c ./openapi/swagger-codegen-cli.cfg.json -t ./openapi/templates -i ./docs/backend.json --skip-validate-spec --additional-properties=ngVersion=12.2.4"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.8",
    "@angular/cdk": "^12.2.8",
    "@angular/common": "~12.2.8",
    "@angular/compiler": "~12.2.8",
    "@angular/core": "~12.2.8",
    "@angular/forms": "~12.2.8",
    "@angular/localize": "^12.2.8",
    "@angular/material": "^12.2.8",
    "@angular/platform-browser": "~12.2.8",
    "@angular/platform-browser-dynamic": "~12.2.8",
    "@angular/router": "~12.2.8",
    "@apollo/client": "^3.4.10",
    "@nebular/eva-icons": "8.0.0",
    "@nebular/theme": "^8.0.0",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@ng-select/ng-select": "^7.3.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "apollo-angular": "^2.6.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "bootstrap": "^5.1.3",
    "disable-scroll": "^0.6.0",
    "eva-icons": "^1.1.3",
    "graphql": "^15.6.1",
    "graphql-tag": "^2.12.5",
    "jwt-decode": "^3.1.2",
    "ng2-completer": "^9.0.1",
    "ng2-smart-table": "^1.7.2",
    "ngx-pagination": "^5.1.1",
    "ngx-scrollbar": "^8.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.1",
    "turndown": "^7.1.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.8",
    "@angular/cli": "~12.2.8",
    "@angular/compiler-cli": "~12.2.8",
    "@schematics/angular": "~12.2.10",
    "@types/jasmine": "~3.10.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^16.11.1",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.10.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ngx-translate-extract": "^1.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~10.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
